{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": ["title", "company_handle"],
  "properties": {
    "title": {
      "type": "string",
      "default": ""
    },
    "salary": {
      "type": "integer",
      "minimum": 0
    },
    "equity": {
      "type": "number",
      "maximum": 1.0
    },
    "company_handle": {
      "type": "string",
      "default": ""
    }
  }
}
